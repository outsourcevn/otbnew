@model List<WebTMDT.Models.ProductHumanType>
@using WebTMDT.Controllers;
<div class="row">
    <div class="col-md-12">
        @if (Model != null)
        {
            if (Model.Count > 0)
            {
                foreach (var item in Model)
                {
                    HomeController hc = new HomeController();
                    if (hc.GetProductOfCat2(item.ProductHumanTypeName))
                    {
                        <div class="panel panel-default panel-category">
                            <div class="panel-heading">
                                <h3 class="panel-title">@item.ProductHumanTypeName</h3>
                            </div>
                            <div class="panel-body">
                                @{Html.RenderAction("_ProductWithCatelog2", new { cat = item.ProductHumanTypeName });}
                            </div>
                        </div>
                    }
                }
            }
        }

    </div>
</div>

